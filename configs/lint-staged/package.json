{
  "name": "@demonicattack/lint-staged",
  "private": true,
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run build:clean && yarn run check-types && tsup",
    "build:clean": "rimraf dist",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .yarn",
    "format": "prettier --check . --cache --cache-location .yarn/cache/.prettiercache",
    "format:fix": "prettier --write . --cache --cache-location .yarn/cache/.prettiercache --log-level=warn",
    "lint": "eslint --cache --cache-location .yarn/cache/.eslintcache",
    "lint:fix": "eslint --fix --cache --cache-location .yarn/cache/.eslintcache"
  },
  "prettier": "./prettier.config.js",
  "dependencies": {
    "lint-staged": "^15.2.10",
    "micromatch": "^4.0.8"
  },
  "devDependencies": {
    "@demonicattack/eslint": "workspace:^",
    "@demonicattack/prettier": "workspace:^",
    "@demonicattack/typescript": "workspace:^",
    "@types/micromatch": "^4.0.9",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@4.5.1"
}
