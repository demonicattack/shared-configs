{
  "name": "@demonicattack/prettier",
  "version": "1.1.1",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run build:clean && yarn run check-types && tsup",
    "build:clean": "rimraf dist",
    "check-types": "tsc --noEmit",
    "clean": "rimraf .yarn node_modules",
    "format": "prettier --check . --no-cache --config prettier.config.js",
    "format:debug": "prettier --check . --no-cache --config prettier.config.js --log-level=debug",
    "format:fix": "prettier --write . --no-cache --config prettier.config.js --log-level=warn",
    "lint": "eslint --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:fix": "eslint --fix --cache --cache-location node_modules/.cache/.eslintcache"
  },
  "dependencies": {
    "local-pkg": "^0.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-multiline-arrays": "^4.0.1",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-sort-json": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.9"
  },
  "devDependencies": {
    "@demonicattack/eslint": "workspace:^",
    "@demonicattack/lint-staged": "workspace:^",
    "@demonicattack/typescript": "workspace:^",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "lint-staged": "^15.2.11",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "prettier": "^3.4.2"
  },
  "packageManager": "yarn@4.5.3",
  "publishConfig": {
    "access": "public"
  }
}
